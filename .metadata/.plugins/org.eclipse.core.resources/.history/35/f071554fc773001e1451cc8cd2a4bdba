		this(0,0);
	}
	
	public SalariedEmployee(double hourlyWage, float hoursWorked) {
		
		this.hourlyWage = hourlyWage;
		this.hoursWorked = hoursWorked;
	}
	
	private void accept() {
		Scanner

	}
	
	
}
