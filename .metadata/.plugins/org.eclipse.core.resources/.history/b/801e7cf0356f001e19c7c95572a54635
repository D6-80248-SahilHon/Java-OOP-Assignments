
public class Pattern1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        int height=5;
        
        
        for(int i=1;i<=5;i++)
        {
        	for(int j=0;j<=5-i;j++)
        	{
        		System.out.println(" ");
        		
        	}
        	for(int k=1;k<=i;k++)
        	{
        		System.out.println("* ");
        	}
        }
	}

}
